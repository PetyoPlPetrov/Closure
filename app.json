{
  "expo": {
    "name": "Closure",
    "slug": "Closure",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "closure",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.petyoplpetrov.Closure"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#101A3D",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "splash": {
        "backgroundColor": "#101A3D",
        "resizeMode": "contain",
        "dark": {
          "backgroundColor": "#101A3D"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.petyoplpetrov.Closure"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#101A3D",
          "dark": {
            "backgroundColor": "#101A3D"
          },
          "resizeMode": "contain"
        }
      ],
      "./plugins/with-transparent-splash-logo.js"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
