{
  "expo": {
    "name": "Sferas",
    "slug": "sphere",
    "version": "1.0.92",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-v2.png",
    "scheme": "sphere",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.petyoplpetrov.Sphere",
      "icon": "./assets/images/app-icon-v2.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "This app uses your photo library to let you select images for your relationships, hobbies, and career profiles. For example, you can pick a photo to represent a family member or add an image to a job entry.",
        "NSPhotoLibraryAddUsageDescription": "The app needs permission to save your exported video memories directly to your photo gallery. For example, you can save an animated recap of your memories to share with friends or keep as a memento.",
        "NSSpeechRecognitionUsageDescription": "This app uses speech recognition to convert your voice input into text for the AI assistant feature.",
        "NSMicrophoneUsageDescription": "This app needs access to your microphone to record your voice for speech-to-text conversion in the AI assistant feature.",
        "UIBackgroundModes": ["processing"]
      },
      "buildNumber": "1.0.92",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1A2332",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "splash": {
        "backgroundColor": "#101A3D",
        "resizeMode": "contain",
        "dark": {
          "backgroundColor": "#101A3D"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.petyoplpetrov.Sphere",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "versionCode": 92,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "buildReactNativeFromSource": true,
            "podfileProperties": {
              "use_modular_headers!": true
            }
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#101A3D",
          "dark": {
            "backgroundColor": "#101A3D"
          },
          "resizeMode": "contain"
        }
      ],
      "expo-notifications",
      "./plugins/with-transparent-splash-logo.js",
      "./plugins/with-firebase-appdelegate",
      "./plugins/withAnalyticsSourcePatch.js",
      "./plugins/with-video-composer.js",
      "./plugins/with-voice.js"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "813af76e-f936-4a41-89f2-7c4f0f9d4732"
      }
    },
    "owner": "petyoplpetrov"
  }
}
